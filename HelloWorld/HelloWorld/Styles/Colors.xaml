<ResourceDictionary xmlns="http://xamarin.com/schemas/2014/forms"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
	<Color x:Key="primary">#aaeeaa</Color>
	<Color x:Key="secondary">#66C5F2</Color>

	<SolidColorBrush Color="{StaticResource primary}" x:Key="primaryBrush"></SolidColorBrush>
	<SolidColorBrush Color="{StaticResource secondary}" x:Key="secondaryBrush"></SolidColorBrush>
</ResourceDictionary>
